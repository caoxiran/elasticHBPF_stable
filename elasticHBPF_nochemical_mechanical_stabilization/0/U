FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    OUTLET
    {
        type            zeroGradient;
    }

    INLET
    {
        type            fixedValue;
        value           uniform (1.1737e-4 0 0);
    }



    
    WALL1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    	   
    }

    WALL2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    	   
    }
    

   
    FRONTBACK1
    {
        type            empty;
    }
    FRONTBACK2
    {
        type            empty;
    }

}

