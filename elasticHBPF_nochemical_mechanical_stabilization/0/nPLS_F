FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nPLS_F;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];//mol m^-3

internalField   uniform 0;

boundaryField
{
    OUTLET
    {
        type            zeroGradient;
    }

    WALL1
    {
        type            zeroGradient;
    }
    WALL2
    {
        type            zeroGradient;
    }



   
    FRONTBACK1
    {
        type            empty;
    }
    FRONTBACK2
    {
        type            empty;
    }



    INLET
    {
        type            zeroGradient;

    }
}

